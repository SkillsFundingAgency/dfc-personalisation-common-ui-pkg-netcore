@model DFC.Personalisation.CommonUI.ViewComponents.Components.Button.ButtonModel

<button id="@Model.Id" class="govuk-button @Model.AdditionalClass @(Model.Disabled ? "govuk-button--disabled" : "")" @(Model.Disabled ? "disabled=disabled aria-disabled=true" : "") data-module="govuk-button">
    @Model.Text
</button>